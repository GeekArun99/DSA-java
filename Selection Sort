public static void SelectionSort(int [] nums){
    for(int i=0; i<nums.length-1; i++){
       int minPos=i;
       for(int j=i+1; j<nums.length; j++){
            if(nums[minPos]>nums[j]{
                minPos=j;
              }
            }
              //swap
            int temp = nums[minPos];
            nums[minPos]=nums[i]
            nums[i]= temp;
     }
}
